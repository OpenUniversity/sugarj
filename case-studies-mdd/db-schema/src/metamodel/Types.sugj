package metamodel;

import metamodel.util.Lexical;

/**
  * copied from https://svn.strategoxt.org/repos/WebDSL/webdsls/trunk/src/org/webdsl/dsl/syntax/WebDSL.sdf
  **/
public sugar Types {
  context-free syntax %% Types
    SimpleSort -> Sort
    GenericSort -> Sort
    RefSort -> Sort
    FunctionSort -> Sort
    
    Id                     -> SimpleSort {cons("SimpleSort")}
    Id "<" {Sort ","}+ ">" -> GenericSort {cons("GenericSort")}
    "Ref" "<" Sort ">" -> RefSort {cons("RefSort")}
    "Ref" "<" Sort ">" -> GenericSort {reject}
}